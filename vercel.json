{
  "rewrites": [
    {
      "source": "/api/:path(.*)",
      "destination": "https://weskhaled-nest.up.railway.app"
    },
    {
      "source": "/graphql/:match*",
      "destination": "https://weskhaled-nest.up.railway.app/graphql"
    },
    {
      "source": "/all-api/:path(.*)",
      "destination": "http://all.api.radio-browser.info/json"
    }
  ]
}
